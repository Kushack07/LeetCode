#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n ;
    cin >> n;
    vector<int>c(n);
    for(int i =0 ; i <c.size();i++){
        cin >> c[i];
    }
    for(int i =c.size()-1; i >=0;i--){     
        cout<<c[i]<<" ";
    }
    
    return 0;
}
